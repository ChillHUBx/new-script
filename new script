--player---------------------

PlayerSection:CreateButton("infiJump", function()
     local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
end)

PlayerSection:CreateButton("Fov 120", function()
   game.Workspace.CurrentCamera.FieldOfView = 120
end)

PlayerSection:CreateButton("Fov reset ♻️", function()
   game.Workspace.CurrentCamera.FieldOfView = 80
end)

--end--

local AimbotTab = CoastingLibrary:CreateTab("Setting")
local MainSection = AimbotTab:CreateSection("Setting")
local RTXSection = AimbotTab:CreateSection("RTX") 

SettingSection:CreateButton("fix lag", function()
   game.Workspace.CurrentCamera.FieldOfView = 80
end)

SettingSection:CreateButton("shift lock", function()
   game.Workspace.CurrentCamera.FieldOfView = 80
end)

