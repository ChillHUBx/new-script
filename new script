local HUBLib = loadstring(game:HttpGet("https://pastebin.com/raw/LpnU0THx"))()

local window = HUBLib:CreateWindow()
local main = window:CreateTab("main", "Main", "Your mom") -- framename, threadname, author

local anotherTab = window:CreateTab("another", "Misc", "Your dad")

anotherTab:CreateButton("Leave", function()  -- buttonname, callback
    game.Players.LocalPlayer:Kick()    
end)

main:CreateButton("Die", function()  -- buttonname, callback
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)

main:CreateToggle("Speed", function(bool) -- togglename, callback
if bool then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
else
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end    
end)


local dropdown = main:CreateDropdown("Teleports", TPlist, function(option) -- dropdownname, list, callback
print(option)    
end)
wait(30)
TPlist = {"Subway", "Spawn"} -- changes the options in the variable of the list
dropdown:Refresh(TPlist) -- refresh dropdown with new options

--[[
¿HOW TO REFRESH A DROPDOWN?
First of all, make ur dropdown a variable, example: local hi = urtab:CreateDropdown("yourmom",{"your dad"}, function(option) print(option) end)

then do hi:Refresh({"new options here"})

¿WHY YOU SHOULD GIVE ME A REP?
jk

thanks Megami Tensei for the idea
]]
