local Tab1 = Window:NewTab("new")
local Tab1Section = Tab1:NewSection("new")

Tab1Section:NewToggle("auto pickup item ?", "click more to stop", function(state)
    if state then
        --auto pickup
_G.on = true --false to turn off and true to turn on
while _G.on do
wait(4) -- you can change the delay here
char = game:GetService('Players').LocalPlayer.Character
game:GetService('Players').LocalPlayer.CharacterAdded:connect(function(body)
char = body end)
for i,v in pairs(workspace:GetDescendants()) do
if v:IsA('Tool') then
char.Humanoid:EquipTool(v) end end
end
    else
        --auto pickup
_G.on = false --false to turn off and true to turn on
while _G.on do
wait(0.4) -- you can change the delay here
char = game:GetService('Players').LocalPlayer.Character
game:GetService('Players').LocalPlayer.CharacterAdded:connect(function(body)
char = body end)
for i,v in pairs(workspace:GetDescendants()) do
if v:IsA('Tool') then
char.Humanoid:EquipTool(v) end end
end
    end
end)

Tab1Section:NewButton("fast reset?","reset", function()
    local teleport_table = {
    location1 = Vector3.new(42,-488,-45), -- your desired position
    location2 = Vector3.new(42,-488,-45)  -- your desired position
}

local tween_s = game:GetService('TweenService')
local tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Linear)

local lp = game.Players.LocalPlayer

function bypass_teleport(v)
    if lp.Character and 
    lp.Character:FindFirstChild('HumanoidRootPart') then
        local cf = CFrame.new(v)
        local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
        a:Play()
        -- a.Completed:Wait()
        -- print('Teleporting Done!')
    end
end

bypass_teleport(teleport_table.location1)
end) 

